# MedicalRecords DApp - Web Frontend Environment Variables

# Contract Configuration (optional - hardcoded fallback exists)
NEXT_PUBLIC_CONTRACT_ADDRESS=0x818FC828b579910ec415d606e4EA34B380cF1d06

# Tenderly RPC (optional - uses MetaMask by default)
NEXT_PUBLIC_RPC_URL=

# Pinata IPFS Gateway (optional - has default)
NEXT_PUBLIC_PINATA_GATEWAY=https://gateway.pinata.cloud/ipfs

# Pinata JWT Token for client-side uploads (REQUIRED)
NEXT_PUBLIC_PINATA_JWT=your_pinata_jwt_token_here

# Pinata JWT Token for server-side (backup, optional)
PINATA_JWT=your_pinata_jwt_token_here
